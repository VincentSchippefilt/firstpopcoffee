@using WebUI.Models
@model List<RoastScheduleViewModel>
@{
    ViewBag.Title = "Roast Planning";
}

<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

@if (@Model.Count > 0)
{
  foreach (var roastSchedule in @Model)
  {
    <ul>
        <li>string.Format("RoastSchedulId: {0}, RoastWeekStartsOn: {1}", @roastSchedule.RoastScheduleId, @roastSchedule.RoastWeekStartsOn)</li>
    </ul>
  }
}
